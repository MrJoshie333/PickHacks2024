{
  "name": "CipherSleuth",
  "description": "A simple extension to decrypt various ciphers within the browser",
  "version": "1.0",
  "manifest_version": 3,
  
  "permissions": [
    "contextMenus"
  ],
  
  "background": {
    "service_worker": "sample.js"
  },

  "action": {
    "default_popup": "hello.html",
    "default_icon": "hello_extensions.png"
  }
}
